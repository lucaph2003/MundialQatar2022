*@
@{
}
@using Dominio;
@model List<Seleccion>



<h1>Listado de Selecciones:</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
        </tr>
    </thead>

    @{
        foreach (Seleccion s in Model)
        {

            <tbody>
                <tr>
                    <th scope="row">i</th>
                    <td>@s.pais.nombre</td>
                    <td></td>
                   
                </tr>
            </tbody>
        }
    }

</table>
