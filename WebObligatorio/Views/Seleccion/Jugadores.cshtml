@{
    ViewData["Title"] = "Jugadores";
}
@using Dominio;
@model List<Jugador>

<h1>Plantel de Seleccion</h1>
<table>
    <thead>
        <tr>
            <th>Dorsal</th>
            <th>Nombre Completo</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (Jugador jugador in Model)
            {
                <tr>
                    <th scope="row">i</th>
                    <td>@jugador.dorsal</td>
                    <td>@jugador.nombreCompleto </td>
                </tr>
            }
        }
        </tbody>
    </table>