@{
}
@using Dominio;
@model List<Resenia>
<h1>Mis Reseñas</h1>


    @{
        foreach (Resenia r in Model)
        {
        <div class="alert alert-primary" role="alert">
            <p><strong>Titulo:</strong> @r.titulo</p>
            <p><strong>Partido:</strong> @r.partido.seleccion1.pais.nombre VS @r.partido.seleccion2.pais.nombre</p>
            <p><strong>Contenido:</strong> @r.contenido</p>
        </div>
        }


    }
